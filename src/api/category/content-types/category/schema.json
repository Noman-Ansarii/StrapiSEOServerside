{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "CategoryTitle": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "CategoryImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "CategorySlug": {
      "type": "uid",
      "targetField": "CategoryTitle",
      "required": true
    },
    "subcategories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subcategorie.subcategorie",
      "mappedBy": "category"
    },
    "posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post.post"
    }
  }
}
